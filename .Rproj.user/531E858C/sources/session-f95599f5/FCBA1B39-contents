
# List the six kind of cards

# Rotationally unique()

card_1 <- rbind(c(1, 1, 0, 0),
                c(1, 1, 0, 0),
                c(0, 0, 1, 0),
                c(0, 0, 0, 1))

card_2 <- rbind(c(1, 0, 0, 0),
                c(0, 1, 0, 0),
                c(1, 0, 1, 0),
                c(0, 0, 0, 1))

card_2 <- rbind(c(1, 1, 0, 0),
                c(1, 1, 0, 0),
                c(0, 0, 1, 1),
                c(0, 0, 1, 1))

